{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base/legacy",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-console":"off"
  }
}
